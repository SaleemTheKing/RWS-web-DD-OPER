<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="models">
    <class name="Bookmark" table="Bookmark">
        <id name="id" type="long" column="id">
            <generator class="native"/>
        </id>
        <property name="userprofile_id" type="long" column="userprofile_id"/>
        <property name="name" type="string" column="name"/>
        <property name="location" type="string" column="location"/>
        <property name="quantity" type="string" column="quantity"/>
        <property name="aspectSet" type="string" column="aspectSet"/>
        <property name="interval" type="string" column="interval"/>
        <property name="startTime" type="string" column="startTime"/>
        <property name="endTime" type="string" column="endTime"/>
    </class>
</hibernate-mapping>
